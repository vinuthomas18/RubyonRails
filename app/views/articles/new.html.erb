<h1>Create an Article</h1>

<%= form_for @article do |f| %>
    <p>
        <%=f.label :title %><br>
        <%=f.text_field :title %>
    </P>
    
    <p>
        <%=f.label :description %><br>
        <%=f.text_area :description %>
    </P>
    
    <p>
        <%=f.submit %>
        
    </P>
    
    <%end%>